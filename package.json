{
  "publisher": "pywind",
  "name": "codeart-vscode-extension",
  "displayName": "CodeArt",
  "description": "Open-Source, Native and a True GitHub Copilot Alternative for VS Code",
  "version": "2.0.0",
  "icon": "assets/logo.png",
  "license": "GPL-3.0-only",
  "pricing": "Free",
  "repository": {
    "type": "git",
    "url": "https://github.com/pywind/codeart-extension"
  },
  "categories": [
    "AI",
    "Chat",
    "Data Science",
    "Education",
    "Testing",
    "Programming Languages",
    "Machine Learning"
  ],
  "keywords": [
    "codeart",
    "free",
    "open",
    "open-source",
    "ai",
    "openai",
    "codex",
    "pilot",
    "snippets",
    "documentation",
    "autocomplete",
    "intellisense",
    "refactor",
    "javascript",
    "python",
    "typescript",
    "php",
    "go",
    "golang",
    "ruby",
    "c++",
    "c#",
    "java",
    "kotlin",
    "co-pilot"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map && node ./build/post-build.mjs",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "test": "vscode-test",
    "format": "prettier --write \"**/*.{ts,mts,cts,tsx,js,mjs,cjs,jsx,json,md}\"",
    "lint": "eslint --config=eslint.config.mjs --fix",
    "prepare": "concurrently \"husky\" \"node ./build/env-setup.mjs\""
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/markdown-it": "^14.1.1",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.61",
    "@types/turndown": "^5.0.5",
    "@types/uuid": "^10.0.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/vsce": "github:mohankumarelec/vscode-vsce#npm",
    "commander": "^12.1.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "node-loader": "^2.0.0",
    "octokit": "^4.0.2",
    "prettier": "3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.12.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "contributes": {
    "walkthroughs": [
      {
        "id": "CodeArt",
        "title": "CodeArt AI",
        "description": "This is a walkthrough for getting started with CodeArt.",
        "icon": "assets/logo.png",
        "steps": [
          {
            "id": "signin",
            "title": "GitHub Sign In",
            "description": "Sign in with GitHub to use CodeArt, your AI pair programmer.\n[Sign In](command:codeart.github.signin)",
            "media": {
              "markdown": "assets/walkthrough.md"
            },
            "completionEvents": [
              "onContext:codeart:walkthroughSignin"
            ]
          },
          {
            "id": "configureModel",
            "title": "Configure Model",
            "description": "Configure the language model provider for CodeArt.\n[Configure](command:codeart.configureModel)",
            "media": {
              "markdown": "assets/walkthrough.md"
            },
            "completionEvents": [
              "onContext:codeart:walkthroughConfigureModel"
            ]
          },
          {
            "id": "panelChat",
            "title": "Start Chatting",
            "description": "Chat with CodeArt in the panel chat view with the configured model.\n[Start Chat](command:workbench.action.chat.open?%7B%22query%22%3A%22What%20model%20do%20you%20use%3F%22%7D)",
            "media": {
              "markdown": "assets/walkthrough.md"
            },
            "completionEvents": [
              "onContext:codeart:walkthroughPanelChat"
            ]
          }
        ]
      }
    ],
    "menus": {},
    "configuration": {
      "type": "object",
      "title": "CodeArt",
      "properties": {
        "codeart.completions.debounceWait": {
          "type": "number",
          "minimum": 10,
          "default": 100,
          "description": "The time in milliseconds to wait before triggering a completion."
        },
        "codeart.completions.contextPrefixWeight": {
          "type": "number",
          "maximum": 0.95,
          "minimum": 0.05,
          "default": 0.85,
          "description": "The proportion of prefix to suffix tokens in the context for completions."
        },
        "codeart.completions.temperature": {
          "type": "number",
          "maximum": 1,
          "minimum": 0,
          "default": 0.1,
          "description": "The randomness of completions, with 0.0 being deterministic and 1.0 being random."
        },
        "codeart.completions.maxTokenUsage": {
          "type": "number",
          "maximum": 128000,
          "minimum": 500,
          "default": 4000,
          "description": "Maximum number of tokens (prompt + output) allowed for generating completions. This will help control the usage cost."
        },
        "codeart.panelChat.temperature": {
          "type": "number",
          "maximum": 1,
          "minimum": 0,
          "default": 0.1,
          "description": "The randomness of panel chat, with 0.0 being deterministic and 1.0 being random."
        },
        "codeart.panelChat.showTokenUsage": {
          "type": "boolean",
          "default": true,
          "description": "Whether to show token usage in panel chat."
        },
        "codeart.inlineChat.temperature": {
          "type": "number",
          "maximum": 1,
          "minimum": 0,
          "default": 0.1,
          "description": "The randomness of inline chat, with 0.0 being deterministic and 1.0 being random."
        },
        "codeart.inlineChat.showTokenUsage": {
          "type": "boolean",
          "default": true,
          "description": "Whether to show token usage in inline chat."
        },
        "codeart.chatSuggestions.temperature": {
          "type": "number",
          "maximum": 1,
          "minimum": 0,
          "default": 0.1,
          "description": "The randomness of chat suggestions, with 0.0 being deterministic and 1.0 being random."
        },
        "codeart.chatTitle.temperature": {
          "type": "number",
          "maximum": 1,
          "minimum": 0,
          "default": 0.1,
          "description": "The randomness of chat title, with 0.0 being deterministic and 1.0 being random."
        },
        "codeart.gitCommitMessage.temperature": {
          "type": "number",
          "maximum": 1,
          "minimum": 0,
          "default": 0.1,
          "description": "The randomness of GIT commit messages, with 0.0 being deterministic and 1.0 being random."
        }
      }
    },
    "chatParticipants": [
      {
        "id": "codeart.panel.default",
        "name": "codeart",
        "fullName": "CodeArt",
        "description": "Ask CodeArt or type / for commands",
        "isDefault": true,
        "locations": [
          "panel"
        ]
      },
      {
        "id": "codeart.editor.default",
        "name": "codeart",
        "fullName": "CodeArt",
        "description": "Ask CodeArt or type / for commands",
        "isDefault": true,
        "locations": [
          "editor"
        ],
        "defaultImplicitVariables": [
          "_inlineChatContext",
          "_inlineChatDocument"
        ]
      }
    ],
    "viewsWelcome": [
      {
        "view": "workbench.panel.chat.view.copilot",
        "contents": "Failed to activate CodeArt. Please check the logs for more information.\n[View Logs](command:codeart.viewLogs)",
        "when": "codeart:isError"
      },
      {
        "view": "workbench.panel.chat.view.copilot",
        "contents": "$(loading~spin) Please wait while CodeArt is getting activated",
        "when": "!codeart:isError && !codeart:isLoaded"
      },
      {
        "view": "workbench.panel.chat.view.copilot",
        "contents": "Sign in with GitHub to use CodeArt, your AI pair programmer.\n[Sign In](command:codeart.github.signin)",
        "when": "!codeart:isError && codeart:isLoaded && !codeart:isLoggedIn"
      }
    ],
    "commands": [
      {
        "command": "codeart.git.generateCommitMessage",
        "title": "Generate Commit Message",
        "icon": "$(sparkle)",
        "enablement": "false",
        "category": "CodeArt"
      },
      {
        "command": "codeart.github.signin",
        "title": "Sign In with GitHub"
      },
      {
        "command": "codeart.configureModel",
        "title": "Configure the Language Model Provider",
        "category": "CodeArt",
        "enablement": "codeart:isLoggedIn"
      },
      {
        "command": "codeart.viewLogs",
        "category": "CodeArt",
        "title": "View logs from CodeArt output channel"
      },
      {
        "command": "codeart.status.icon.menu",
        "enablement": "false",
        "title": "Status Icon Menu"
      }
    ],
    "icons": {
      "codeart-default": {
        "description": "CodeArt Default Logo",
        "default": {
          "fontPath": "assets/fonts.woff",
          "fontCharacter": "\\e900"
        }
      },
      "codeart-disabled": {
        "description": "CodeArt Disabled Logo",
        "default": {
          "fontPath": "assets/fonts.woff",
          "fontCharacter": "\\e901"
        }
      }
    },
    "iconFonts": [
      {
        "id": "codeart-font",
        "src": [
          {
            "path": "assets/fonts.woff",
            "format": "woff"
          }
        ]
      }
    ]
  },
  "extensionKind": [
    "ui"
  ],
